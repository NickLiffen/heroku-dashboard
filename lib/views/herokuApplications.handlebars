                        <a href="/" class="breadcrumb black-text">Home</a>
                        <a href="/application" class="breadcrumb black-text">Applications</a>

                    </div>
                </div>
        </div>
    </div>


    <div class='container padding-top'>
        <div class='row'>
            <div class="col s9">
                <h3 class='center'>Heroku Applications</h3>
            </div>
            <div class="col s3">
                <i class="material-icons">search</i>
                <input type="text" id='searchApps' class="form-control" placeholder="Search for Application...">
                <span class="input-group-btn">
    </span>
            </div>
        </div>
        <div class='row'>
            <div id="HerokuApps">
                <table class="striped centered responsive-table" id='herokuAppTable'>
                    <thead>
                        <th>Application Name</th>
                        <th>Application Profile</th>
                    </thead>
                </table>
            </div>
        </div>
    </div>


    <div class="container">
      <div class="row">
         <div class="col s8">
        <canvas id="lineChart" width="100" height="100"></canvas>
      </div>
    </div>
  </div>

